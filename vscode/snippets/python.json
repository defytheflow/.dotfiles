{
  "ipython debugger": {
    "prefix": ["ipdb"],
    "body": ["__import__('ipdb').set_trace()"]
  },

  "print debug": {
    "prefix": ["pd"],
    "body": ["print(f'${1} is {${1}}')"]
  },

  "pretty print": {
    "prefix": ["pp"],
    "body": ["__import__('pprint').pprint($1)"]
  },

  "logging info": {
    "prefix": ["info"],
    "body": ["__import__('logging').info($1)"]
  },

  "logging warning": {
    "prefix": ["warn"],
    "body": ["__import__('logging').warning($1)"]
  },
}
