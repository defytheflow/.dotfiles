#!/bin/bash

# -----------------------------------------------------------------------------
# | File:      .bash_profile
# -----------------------------------------------------------------------------
# | Modified:  March 17, 2020
# ---------------------------------------------------------------------------- 
# | Author:    Artyom Danilov
# -----------------------------------------------------------------------------

echo 'Running .bash_profile'

# If bash is installed:
if [[ -n "${BASH_VERSION}" ]]; then
    if [[ -f "${HOME}/.bashrc" ]]; then
        . "${HOME}/.bashrc"
    fi
fi
